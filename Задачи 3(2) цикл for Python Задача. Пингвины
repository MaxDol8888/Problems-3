v = int(input())
a1 = '    _~_    '
a2 = '   (o o)   '
a3 = '  /  V  \  '
a4 = ' /(  _  )\ '
a5 = '   ^^ ^^   '
 
print(a1*v)
print(a2*v)
print(a3*v)
print(a4*v)
print(a5*v)


a= int(input())
b= '    _~_    '
c= '   (o o)   '
d= '  /  c  \  '
e= ' /(  _  )\ '
f= '   ^^ ^^   '
 
print(b*a)
print(c*a)
print(d*a)
print(e*a)
print(f*a)


